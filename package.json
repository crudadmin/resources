{
  "name": "crudadmin",
  "description": "CrudAdmin",
  "version": "2.0.0",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-web": "npm run development --section=web -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production --section=web && npm run production --section=admin --kill-others-on-fail",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "precommit": "npm run prod && git add src/Resources/admin/js/* && git add src/Resources/admin/css/*"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "animate.css": "^3.7.2",
    "git-pre-commit": "^2.1.4",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.23.6",
    "sass-loader": "^7.3.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "cross-env": "^5.2.1",
    "jquery": "^3.4.1",
    "jquery-datetimepicker": "^2.5.21",
    "jquery-form": "^3.5.0",
    "js-md5": "^0.7.3",
    "laravel-mix": "^4.1.4",
    "lodash": "^4.17.15",
    "moment": "^2.20.1",
    "popper.js": "^1.12",
    "vue": "^2.6.11",
    "vue-fragment": "^1.5.1",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.11",
    "vuedraggable": "^2.23.2"
  }
}
