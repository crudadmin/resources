{
  "name": "crudadmin",
  "description": "CrudAdmin",
  "version": "3.0.0",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-web": "npm run watch --section=web",
    "hot": "mix watch --hot",
    "prod": "npm run production --section=admin && npm run production --section=web --kill-others-on-fail",
    "production": "mix --production",
    "precommit": "npm run prod && git add src/Resources/admin/js/* && git add src/Resources/admin/css/*"
  },
  "devDependencies": {
    "git-pre-commit": "^2.1.4",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "sass-resources-loader": "^2.2.5",
    "targets-webpack-plugin": "^3.0.0",
    "vue-loader": "^15.9.8"
  },
  "dependencies": {
    "@babel/preset-react": "^7.16.7",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "animate.css": "^3.7.2",
    "bootstrap": "^4.6.1",
    "gettext-translator": "^3.0.1",
    "jquery": "^3.6.0",
    "jquery-datetimepicker": "^2.5.21",
    "jquery-form": "^3.5.0",
    "js-md5": "^0.7.3",
    "laravel-mix": "^6.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "popper.js": "^1.16.1",
    "postcss": "^8.4.12",
    "verte": "0.0.12",
    "vue": "^2.6.14",
    "vue-fragment": "1.5.1",
    "vue-resource": "^1.5.3",
    "vue-router": "^3.5.3",
    "vue-tel-input": "^5.11.0",
    "vue-template-compiler": "^2.6.14",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2"
  }
}
